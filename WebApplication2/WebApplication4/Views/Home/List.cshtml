@model IList<WebApplication4.Models.person>


<table border="2">
	<tr>
		<th>First name</th>
		<th>Last Name</th>
		<th>DOB</th>
		<th>Email ID</th>
		<th>Mobile Number</th>
		<th>Gender</th>
		<th>Address</th>
		<th>City</th>
		<th>Pin Code</th>
		<th>State</th>
		<th>Country</th>
		<th>Hobbies</th>
		<th>Courses applied for</th>


	</tr>
	@foreach (var i in Model)
	{
		<tr>
			<td>@i.FirstName</td>
			<td>@i.LastName</td>
			<td>@i.Date / @i.month / @i.year</td>
			<td>@i.Email</td>
			<td>@i.MobileNumber</td>
			<td>@i.Gender</td>
			<td>@i.Address</td>
			<td>@i.City</td>
			<td>@i.pincode</td>
			<td>@i.state</td>
			<td>@i.coun</td>
			<td>@(i.Hobb != null ? string.Join(", ", i.Hobb) : "")</td>
			<td>@i.courses</td>
		</tr>
	}


</table>
<h2 class="mt-5">Qualification</h2>
<table border="2">
	<tr>
		<th colspan="3">Qualification (ClassX)</th>
		<th colspan="3">Qualification (ClassXII)</th>
		<th colspan="3">Qualification (Graduation)</th>
		<th colspan="3">Qualification (Masters)</th>
	</tr>
	<tr>
		<td>Board</td>
		<td>Percentage</td>
		<td>Year of Passing</td>
		<td>Board</td>
		<td>Percentage</td>
		<td>Year of Passing</td>
		<td>Board</td>
		<td>Percentage</td>
		<td>Year of Passing</td>
		<td>Board</td>
		<td>Percentage</td>
		<td>Year of Passing</td>
	</tr>
	@foreach (var i in Model)
	{
		<tr>
			<td>@i.classXB</td>
			<td>@i.classXP</td>
			<td>@i.classXY</td>
			<td>@i.classXIIB</td>
			<td>@i.classXIIP</td>
			<td>@i.classXIIY</td>
			<td>@i.GraduationB</td>
			<td>@i.GraduationP</td>
			<td>@i.GraduationY</td>
			<td>@i.MastersB</td>
			<td>@i.MastersP</td>
			<td>@i.MastersY</td>
		</tr>
	}
</table>
